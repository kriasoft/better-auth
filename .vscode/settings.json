{
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "cSpell.words": [
    "bunx",
    "konstantin",
    "kriasoft",
    "namespacing",
    "onedrive",
    "rollout",
    "rollouts",
    "stringbool",
    "tarkus",
    "vitepress"
  ],
  "search.exclude": {
    "vendor": true
  }
}
